    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "prettier": "2.8.3",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/00f2ff/result.git"
